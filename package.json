{
  "name": "rrb",
  "version": "0.0.1",
  "description": "React Redux Boilerplate for hiring challenges",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run prod",
    "dev": "better-npm-run dev",
    "gen": "babel-node bin/generate-component.js"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -i app -x babel-node server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "babel-node ./bin/build.js && babel-node server.js",
      "env": {
        "NODE_ENV": "production",
        "HOST": "0.0.0.0"
      }
    }
  },
  "author": "Artem Platonov",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "better-npm-run": "0.0.9",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "express-http-proxy": "^0.7.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.9.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.5.3",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.8.1",
    "precss": "^1.4.0",
    "sass-loader": "^4.0.0",
    "shelljs": "^0.7.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.14.1",
    "normalize.css": "^4.2.0",
    "react": "^0.14.8",
    "react-copy-to-clipboard": "^4.2.1",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-form": "^5.3.1",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  }
}
